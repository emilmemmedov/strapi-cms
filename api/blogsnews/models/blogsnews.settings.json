        "i18n": {
          "localized": true
        }
      }
    },
    "pageName": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "startDate": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": true
