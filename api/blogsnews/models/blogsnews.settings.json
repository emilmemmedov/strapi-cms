          "localized": true
        }
      }
    },
    "facebookMetaTags": {
      "type": "component",
      "repeatable": false,
      "component": "shared.facebook-meta-tags",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "twitterMetaTags": {
      "type": "component",
      "repeatable": false,
